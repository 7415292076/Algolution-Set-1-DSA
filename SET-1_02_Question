#include <iostream>
using namespace std;
int main() {
   int max;
   int arr[5];
   cout<<"\nEnter Value of array ..\n";
   for(int i = 0 ; i<5 ; i++)
   {
       cout<<"Enter value of "<< i <<" index ";
       cin>>arr[i];
   }
    max=arr[0];
    for(int i = 1 ; i<5 ; i++)
   {
      if(max < arr[i])
      {
          max = arr[i];
      }
      else
      {
          
      }
   }
   cout<<"The maximum number of ARAY is "<< max;
   }


