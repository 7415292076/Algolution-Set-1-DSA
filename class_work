#include <iostream>
using namespace std;

int main() {
    //find the majority element (element appres more then n/2) arr=3,3,4,2,4,,2,4,4,4
    int arr[]={3,3,4,2,4,4,2,4,4};
   int k=0,l;
   int m = 9/2;
  for(int i=0; i<=8; i++)
  {
    for(int j=1; j<=8; j++)  
    {
        if(arr[i]==arr[j])
        {
            k++ ;
            if(m<=k)
            {
                l=arr[i];
            //  cout<<arr[i]  ; 
             break;   
            }
            else
            {
            }
        }
        else
        {
        }
    }
    k=0;
  }
  cout<<l;
    return 0;
}
